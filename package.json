{
	"name": "apricot",
	"version": "0.2.15",
	"private": true,
	"scripts": {
		"build": "next build",
		"build:test": "NEXT_PUBLIC_APRICOT_ENV=test APRICOT_ENV=test next build",
		"check:ts": "tsc --noEmit",
		"cypress": "cypress open",
		"cypress:setup": "npm run db:setup && npm run storage:start",
		"cypress:start": "NEXT_PUBLIC_APRICOT_ENV=test APRICOT_ENV=test npm run next:dev",
		"cypress:run": "cypress run --e2e",
		"cypress:component": "cypress run --component",
		"db:hard-reset": "npm run db:remove && npm run db:setup",
		"db:remove": "docker stop postgres-db || true && docker rm postgres-db || true",
		"db:seed": "prisma db seed",
		"db:setup": "npm run db:start && npm run generate-and-push && npm run db:seed",
		"db:start": "docker run --name postgres-db -d -e POSTGRES_DB=apricot -e POSTGRES_PASSWORD=testpass123 -e POSTGRES_USER=postgres -p '5432:5432' postgres",
		"dev:test": "NEXT_PUBLIC_APRICOT_ENV=test APRICOT_ENV=test npm run dev:base",
		"dev": "NODE_ENV=development npm run dev:base",
		"dev:base": "npx concurrently --names next,prismagen,prismastudio,inggest,compile --prefix-colors white,green,blue,magenta,cyan 'npm run next:dev' 'npm run generate-and-push:watch' 'npm run studio' 'npm run inngest:dev' 'npm run check:ts'",
		"format:check": "prettier -c .",
		"format:write": "prettier --write .",
		"generate-and-push": "prisma generate && prisma db push",
		"generate-and-push:watch": "npx nodemon --watch prisma/schema.prisma --exec npm run generate-and-push",
		"inngest:dev": "npx inngest-cli@latest dev",
		"lint": "next lint",
		"migrate": "prisma migrate dev",
		"next:dev": "next dev",
		"postinstall": "prisma generate",
		"pull": "prisma db pull",
		"setup:env": "cp .env.example .env",
		"setup": "npm i && npm run setup:env && npm run db:hard-reset && npm run storage:reset",
		"start": "next start",
		"start:test": "NEXT_PUBLIC_APRICOT_ENV=test APRICOT_ENV=test node .next/standalone/server.js",
		"storage:remove": "docker stop fake-gcs-server || true && docker rm fake-gcs-server || true",
		"storage:reset": "npm run storage:remove && npm run storage:start",
		"storage:start": "docker run -d --name fake-gcs-server -p 4443:4443 -v ${PWD}/test/sample-data:/data fsouza/fake-gcs-server -scheme http -external-url http://localhost:4443 -backend memory -public-host localhost:4443",
		"studio": "prisma studio",
		"update-dependencies": "npx npm-check-updates -u",
		"test": "NEXT_PUBLIC_APRICOT_ENV=test APRICOT_ENV=test jest --detectOpenHandles",
		"test:external": "NEXT_PUBLIC_APRICOT_ENV=test APRICOT_ENV=test jest --detectOpenHandles --config jest.external.config.ts",
		"test:watch": "NEXT_PUBLIC_APRICOT_ENV=test APRICOT_ENV=test jest --no-coverage --watch --detectOpenHandles",
		"configs:download": "npx vite-node src/assessments/home-care-home-base/orgs/downloadAllOrgConfigs && npm run format:write",
		"configs:add-to-db": "npx vite-node src/assessments/scripts/mainAddConfigurationToDB",
		"release:log": "git log origin/main..origin/staging --oneline --reverse --pretty=format:%s"
	},
	"dependencies": {
		"@anthropic-ai/sdk": "^0.24.0",
		"@emotion/cache": "^11.11.0",
		"@emotion/react": "^11.11.1",
